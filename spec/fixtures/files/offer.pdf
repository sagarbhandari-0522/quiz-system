        /***phone number format***/
        $('.phone-format').keydown(function (e) {
            if ($(window).width() <= 1024) {

                $('.phone-format').attr('type', 'tel');
                $(this).attr('maxlength', '10');
            }
            else {
                $('.phone-format').attr('type', 'text');
                $(this).attr('maxlength', '14');
                key = e.which || e.charCode || e.keyCode || 0;
                $phone = $(this);
                if ($phone.val().length === 0) {
                    $phone.val('(');
                }
                if ($phone.val().length === 1 && (key === 8 || key === 46)) {
                    $phone.val('');
                    return false;
                }
                else if ($phone.val().charAt(0) !== '(') {
                    $phone.val('(' + String.fromCharCode(e.keyCode) + '');
                }
                if (key !== 8 && key !== 9) {
                    if ($phone.val().length === 4) {
                        $phone.val($phone.val() + ')-');
                    }
                    if ($phone.val().length === 5) {
                        if (key != 8 && key != 0 && key != 46 && (key < 48 || key > 57)) { return false; }
                        else {
                            $phone.val($phone.val() + '-');
                        }

                    }
                    if ($phone.val().length === 9) {
                        $phone.val($phone.val() + '-');
                    }
                }

                if (key != 8 && key != 0 && key != 46 && (key < 48 || key > 57)) {

                    return false;
                }
                else {
                    var val = $phone.val();
                    $phone.val('').val(val);
                }
                return (key == 8 ||
                    key == 9 ||
                    key == 46 ||
                    (key >= 48 && key <= 57) ||
                    (key >= 96 && key <= 105));
            }

