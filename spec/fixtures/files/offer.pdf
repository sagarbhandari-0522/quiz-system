<script type="text/javascript">
    var text_title = " ";
    //var imageLoader = document.getElementById('imageLoader');
    //imageLoader.addEventListener('change', handleImage, false);
    var canvas = document.getElementById('imageCanvas');
    var ctx = canvas.getContext('2d');
    var img = new Image();
    img.crossOrigin = "anonymous";
    document.getElementById('typein').addEventListener('keyup', function () {
        ctx.clearRect(0, 0, canvas.width, canvas.height);
        //DrawOverlay(img);
        text_title = this.value;
        DrawText();
    });

    function DrawText() {
        ctx.fillStyle = "black";
        ctx.textBaseline = 'middle';
        ctx.font = "30px 'Segoe', cursive";
        ctx.fillText(text_title, 30, 40);
    }

    //For Image upload
    function onChangeImageUploadFromModalControl() {
        var imageUpload = $('#imageUploadFromModalControl');
        $('#progress-modal').modal('show');
        var formData = new FormData();
        formData.append(imageUpload[0].files[0].name, imageUpload[0].files[0]);
        var fname = imageUpload[0].files[0].name;
        var unqn = generateUinqueIDentifier()
        unqn = unqn.split('-').join('');
        unqn = fname.replace('.', '_' + unqn + '.')
        var userID = getUserGuid();
        var uploadQS = "&unqn=" + unqn + "&foln=" + userID;
        var iurl = "/account/image-upload.ashx?" + uploadQS;
        $.ajax({
            type: "POST",
            contentType: false,
            processData: false,
            url: iurl,
            data: formData,
            timeout: document.getElementById('ClientSideAjaxTimeOut').value,
            success: function (result) {
                $('#progress-modal').modal('hide');
            },
            error: function (err) {
                $('#progress-modal').modal('hide');
                alert(err.statusText);
                $('#digitalSignatureContainer').remodal().close();
            }
        });
    };


    $('#imageUploadFromModalControl').change(function () {
        var filename = $('#imageUploadFromModalControl')[0].files[0].name;
        document.getElementById('errorMsg').style.display = "";
        document.getElementById('errorMsg').style.color = "black";
        document.getElementById('errorMsg').text = filename;
    });

